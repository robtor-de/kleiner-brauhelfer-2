               <item>
                <widget class="QLabel" name="label_60">
                 <property name="text">
                  <string>Zusätze &quot;Gärung&quot;</string>
                 </property>
                </widget>
               </item>
               <item>
                <spacer name="horizontalSpacer_10">
                 <property name="orientation">
                  <enum>Qt::Horizontal</enum>
                 </property>
                 <property name="sizeHint" stdset="0">
                  <size>
                   <width>40</width>
